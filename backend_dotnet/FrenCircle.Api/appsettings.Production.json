{
  "Serilog": {
    "WriteTo": [
      {
       "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/fren-circle-.log",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 14,
          "fileSizeLimitBytes": 10485760,
          "rollOnFileSizeLimit": true,
          "shared": true,
          "outputTemplate": "{Timestamp:O} [{Level:u3}] {SourceContext} {Message:lj} {Properties:j}{NewLine}{Exception}"
        }
      }
    ]
  }
}
